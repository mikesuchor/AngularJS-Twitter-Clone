{
    "env": {
        "es6": false
    },
    "extends": "google",
    "rules": {
        "no-var": [
            "off"
        ],
        "comma-dangle": [
            "error",
            "never"
        ],
        "require-jsdoc": [
            "off"
        ],
        "valid-jsdoc": [
            "off"
        ],
        "max-len": [
            "error", 
            {
                "code": 120
            }
        ],
        "switch-colon-spacing": [
            "off"
        ],
        "linebreak-style": [
            "error",
            "windows"
        ]
    }
}